# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
ADMIN_USER_ID=your_telegram_user_id
# Reviewers - can edit titles, reschedule (comma separated)
REVIEWER_USER_IDS=
# Mods - can upload videos (comma separated)
MOD_USER_IDS=

# Redis configuration (optional)
REDIS_HOST=localhost
REDIS_PORT=6379

# Git Token for auto-sync (optional)
GIT_TOKEN=

# Supabase PostgreSQL Database
DATABASE_URL="postgresql://postgres.[PROJECT_REF]:[PASSWORD]@aws-1-ap-southeast-1.pooler.supabase.com:6543/postgres?pgbouncer=true"
DIRECT_URL="postgresql://postgres.[PROJECT_REF]:[PASSWORD]@aws-1-ap-southeast-1.pooler.supabase.com:5432/postgres"

# Supabase S3 Storage
S3_ENDPOINT=https://[PROJECT_REF].storage.supabase.co/storage/v1/s3
S3_REGION=ap-southeast-1
S3_ACCESS_KEY_ID=your_s3_access_key_id
S3_SECRET_ACCESS_KEY=your_s3_secret_access_key
S3_BUCKET=videos